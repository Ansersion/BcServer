<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://www.hibernate.org/dtd/hibernate-mapping">  
 
<hibernate-mapping package="db">    
    <class name="SnInfoHbn" table="sn_info">  
        <id name="id" column="id">  
            <generator class="native" />
        </id>  
        <property name="sn" column="sn" />  
        <property name="developUserId" column="develop_user_id" />  
        <property name="activiteDate" column="activite_date" />  
        <property name="expiredDate" column="expired_date" />  
        <property name="existTime" column="exist_time" />  
    </class>    
</hibernate-mapping>