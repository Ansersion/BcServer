<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
        "http://www.hibernate.org/dtd/hibernate-mapping">  
 
<hibernate-mapping package="db">    
    <class name="CustomSignalStringDefaultValueEntityInfoHbn" table="custom_signal_string_default_value_entity_info">  
        <id name="id" column="id">  
            <generator class="native" />
        </id>  
        <property name="chinese" column="chinese" />  
        <property name="english" column="english" />  
        <property name="french" column="french" />  
        <property name="russian" column="russian" />  
        <property name="arabic" column="arabic" />  
        <property name="spanish" column="spanish" />  
    </class>    
</hibernate-mapping>