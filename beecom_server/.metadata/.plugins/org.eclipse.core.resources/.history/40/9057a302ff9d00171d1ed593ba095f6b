/**
 * 
 */
package bc_server;

/**
 * @author Ansersion
 *
 */
public class Payload {

	byte[] UserName = null;
	byte[] Password = null;
	
	public void setUserName(byte[] user_name) {
		UserName = user_name;
	}
	
	public void setPassword(byte[] password) {
		Password = password;
	}
	
	public void reset() {
		UserName = null;
		Password = null;
	}
}
