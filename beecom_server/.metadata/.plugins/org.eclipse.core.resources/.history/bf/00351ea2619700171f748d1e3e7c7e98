package bc_server;

public class CrcType {
    /**
     * Represents the CRC type
     * of no encryption
     */
    public static final CrcType CRC16 = new CrcType("CRC16");
    public static final CrcType CRC32 = new CrcType("CRC32");

    private final String strValue;

    /**
     * Creates a new instance.
     */
    private CrcType(String strValue) {
        this.strValue = strValue;
    }

    /**
     * @return the string representation of this encrpytion type
     * <ul>
     *   <li>{@link #CRC16} - <tt>"no encryptioin"</tt></li>
     *   <li>{@link #CRC32} - <tt>"no encryptioin"</tt></li>
     * </ul>
     */
    @Override
    public String toString() {
        return strValue;
    }

}
